<?xml version="1.0" encoding="utf-8"?>
<project.myshulteproject.com.util.ExpandLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/el_odd_number"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <TextView
            android:id="@+id/tv_odd_number_3"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="3 X 3"
            android:textColor="#999"
            android:textSize="18dp" />

        <TextView
            android:layout_width="180dp"
            android:layout_height="0.5dp"
            android:background="#999"
            tools:ignore="Suspicious0dp" />

        <TextView
            android:id="@+id/tv_odd_number_5"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="5 X 5"
            android:textColor="#999"
            android:textSize="18dp" />

        <TextView
            android:layout_width="180dp"
            android:layout_height="0.5dp"
            android:background="#999"
            tools:ignore="Suspicious0dp" />

        <TextView
            android:id="@+id/tv_odd_number_7"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="7 X 7"
            android:textColor="#999"
            android:textSize="18dp" />

        <TextView
            android:layout_width="180dp"
            android:layout_height="0.5dp"
            android:background="#999"
            tools:ignore="Suspicious0dp" />

    </LinearLayout>
</project.myshulteproject.com.util.ExpandLayout>